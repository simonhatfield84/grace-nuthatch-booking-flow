# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_PUBLISHABLE_KEY=your-anon-key-here
VITE_SUPABASE_PROJECT_ID=your-project-id

# Refactoring Feature Flags
# Master toggle - when true, enables all new refactored code paths
VITE_REFRACTOR_MODE=false

# Uses optimized availability calculation
VITE_NEW_AVAILABILITY_ENGINE=false

# Enforces strict input/output validation (recommended: true)
VITE_CONTRACT_VALIDATION=true

# Exposes internal state for testing (development only)
VITE_SMOKE_TEST_MODE=false
